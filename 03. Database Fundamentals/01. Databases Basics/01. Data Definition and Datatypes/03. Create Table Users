CREATE TABLE Users
(
[id] int PRIMARY KEY IDENTITY(1,1),
[username] nvarchar(30) NOT NULL,
[password] nvarchar(26) NOT NULL,
[profilePicture] varbinary(max),
[lastLoginTime] date,
[isDeleted] varchar(1)
);

