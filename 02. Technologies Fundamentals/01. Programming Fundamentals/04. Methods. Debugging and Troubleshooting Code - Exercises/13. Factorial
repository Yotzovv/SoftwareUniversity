using System;
using System.Collections.Generic;
using System.Linq;
using System.Numerics;

public class GeometryCalculator
{
    public static void Main()
    {
        BigInteger factorial = 1;
        int n = int.Parse(Console.ReadLine());

        for (int i = 1; i <= n;i++)
        {
            factorial *= i;
        }
        Console.WriteLine(factorial);
    }
}
