using System;

class InstructionSet_broken
{
    static void Main()
    {
        sbyte first = -100;
        byte second = 128;
        short third = -3540;
        int fourt = 64876;
        uint fifth = 2147483648;
        long sixt = -1141583228;
        long seventh = -1223372036854775808;

        Console.WriteLine(first);
        Console.WriteLine(second);
        Console.WriteLine(third);
        Console.WriteLine(fourt);
        Console.WriteLine(fifth);
        Console.WriteLine(sixt);
        Console.WriteLine(seventh);
    }
}