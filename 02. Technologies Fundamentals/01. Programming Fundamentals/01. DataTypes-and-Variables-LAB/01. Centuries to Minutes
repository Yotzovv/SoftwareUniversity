using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace TechModuleFirstExam
{
    class Program
    {
        static void Main(string[] args)
        {
            var centuries = byte.Parse(Console.ReadLine());
            var years = (int)centuries * 100;
            var days = Math.Round(years * 365.2422F);
            var hours = days * 24;
            var mins = hours * 60;

            Console.WriteLine("{0} centuries = {1} years = {2} days = {3} hours = {4} minutes", centuries, years, days, hours, mins);
        }
    }
}
