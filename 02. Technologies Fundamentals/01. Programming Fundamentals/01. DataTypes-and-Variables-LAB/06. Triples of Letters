using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace TechModuleFirstExam
{
    class Program
    {
        static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());

            for (int i = 1; i <= n; i++)
            {
                for (int e = 1; e <= n; e++)
                {
                    for (int c = 1; c <= n; c++)
                    {
                        Console.WriteLine("{0}{1}{2}", (char)(i+96), (char)(e+96), (char)(c+96));
                    }
                }
            }
        }
    }
}