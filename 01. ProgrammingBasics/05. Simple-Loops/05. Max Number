using System;
using System.Collections.Generic;

namespace HW
{
    class MainClass
    {
        public static void Main(string[] args)
        {
            int n = int.Parse(Console.ReadLine());
            var max = -100000;
            int sum = 0;

            for(int i = 0;i < n;i++)
            {
                int nums = int.Parse(Console.ReadLine());
                if(nums > max)
                {
                    max = nums;
                }
            }
            Console.Write(max);
        }
    }
}