using System;
using System.Collections.Generic;
using System.Linq;

namespace asdasdasd
{
	class MainClass
	{
		static void Main (string[] args)
		{
			string product = Console.ReadLine ();
			string[] fruit = {"banana", "apple", "kiwi", "cherry", "lemon", "grapes"};
			string[] vegetables = {"tomato", "cucumber", "pepper", "carrot"};

			if (fruit.Contains(product)) {Console.WriteLine("fruit");}
			else if (vegetables.Contains(product)) {Console.WriteLine("vegetable");}
			else {Console.WriteLine("unknown");}
			}
		}
	}
