using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Collections;

namespace AdvancedLoops
{
	class Nto1
	{
		static void Main (string[] args)
		{
			int n = int.Parse(Console.ReadLine());
			int i = n;

			while(i != 1 && i > 1)
			{
				Console.WriteLine (i);
				i--;
			}
			Console.WriteLine (i);
		}
	}
}
