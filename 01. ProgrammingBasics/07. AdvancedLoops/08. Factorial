using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Collections;

namespace AdvancedLoops
{
	class CGD
	{
		static void Main (string[] args)
		{
			int n = int.Parse(Console.ReadLine());
			int factoriel = 1;

			for(int i = 1;i <= n;i++)
			{
				factoriel *= i;
			}
			Console.WriteLine (factoriel);
		}
	}
}