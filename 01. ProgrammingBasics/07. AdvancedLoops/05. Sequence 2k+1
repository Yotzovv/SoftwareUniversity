using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Collections;

namespace AdvancedLoops
{
	class Nto1
	{
		static void Main (string[] args)
		{
			int n = int.Parse(Console.ReadLine());
			int nums = 1;

			for(int i = 0;i <= n;i*=2)
			{
				i++;
				Console.WriteLine(i);
			}
		}
	}
}
