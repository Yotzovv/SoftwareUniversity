using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Collections;

namespace Snake
{
	class Program
	{
		static void Main (string[] args)
		{
			int n = int.Parse(Console.ReadLine());
			int i = 1;

			while(i != n && i < n)
			{
				Console.WriteLine(i);
				i+=3;

			}
			if (i == n) {
				Console.WriteLine (i);

			}
		}
	}
}